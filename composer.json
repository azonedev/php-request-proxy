            "email": "nahid.dns@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "laminas/laminas-diactoros": "^2.0",
        "relay/relay": "^1.0",
        "laminas/laminas-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.1"
